void setup() 
{ 
  Serial.begin(115200); // initialize serial communication at 115200 bits per second
  buccalservo.attach(9);  
  operservo.attach(5);
  brmservo.attach(6); 
} 
 
 
void loop() 
{ 
  unsigned long curmillis = millis();
  unsigned long curmillisservo = millis();
  
  if(curmillis - prevmillis > pressureinterval){
    prevmillis = curmillis;
     int buccalpressure = analogRead(A0);
     int operpressure = analogRead(A1);  
     Serial.print("Buccal: ");
     Serial.print(buccalpressure);
     Serial.print("\t Opercular: ");
     Serial.print(operpressure);
     Serial.print("\t Buccal Servo: ");
     Serial.print(buccal180array[oldarray]); 
     Serial.print("\t Opercular Servo: ");
     Serial.print(oper180array[oldarray]);
     Serial.print("\t BRM Servo: ");
     Serial.print(oper180array[oldarray]);  
     Serial.print("\t Time: ");
     Serial.println(curmillis); //prints time since program started
  
  if(curmillisservo - prevmillisservo > servointerval){
    prevmillisservo = curmillisservo;
    
    int newarray = oldarray + 1;   //new position of servo array
    
    if(newarray - oldarray > 0){
      
      for(pos = oldarray; pos < newarray; pos++)   //pos = array value; in steps of one array position
      {                                
        buccalservo.write(buccal180array[pos]);  //calling array values using pos variable    
        operservo.write(oper180array[pos]);
        brmservo.write(oper180array[pos]);
        
        if(newarray < 179){   
          
          oldarray = newarray;
        }
        
        else{
          oldarray = 0;
        }
      }  
    }
  }
  }  
  
  
} 
